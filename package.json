{
  "name": "intersystems-serverdiff",
  "displayName": "InterSystems Server QuickDiff",
  "description": "Show differences between current document and server version",
  "publisher": "georgejames",
  "version": "0.0.1",
  "license": "MIT",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
		"intersystems",
		"objectscript"
  ],
  "icon": "media/extension-icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/gjsjohnmurray/vscode-intersystems-serverdiff"
  },
  "enabledApiProposals": [
    "quickDiffProvider"
  ],
  "extensionDependencies": [
    "intersystems-community.vscode-objectscript"
  ],
  "activationEvents": [
    "onLanguage:objectscript",
    "onLanguage:objectscript-class",
    "onLanguage:objectscript-int",
    "onLanguage:objectscript-macros",
    "onFileSystem:isfs"
  ],
  "main": "./dist/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "16.x",
		"@types/semver": "^7.3.13",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "eslint": "^8.24.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "ts-loader": "^9.4.1",
    "typescript": "^4.8.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "@vscode/test-electron": "^2.1.5"
  }
}
